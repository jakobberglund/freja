{
  "version": 2,
  "projects": {
    "components": {
      "projectType": "library",
      "generators": {
        "@nxext/stencil:component": {
          "style": "scss",
          "storybook": false
        }
      },
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "targets": {
        "test": {
          "executor": "@nxext/stencil:test",
          "options": {
            "projectType": "library",
            "configPath": "libs/components/stencil.config.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nxext/stencil"
  }
}